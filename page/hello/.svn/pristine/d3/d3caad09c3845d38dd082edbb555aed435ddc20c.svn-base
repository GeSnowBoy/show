  animation: city1-action 9s linear 1 forwards;
}

.city2.active {
  -webkit-animation: city2-action 8s linear 1 forwards;
  animation: city2-action 8s linear 1 forwards;
}

.text-describe {
  transform: translate3d(0, -200%, 0);
  -webkit-transform: translate3d(0, -200%, 0);
  transition: 5s all;
}

.text-describe.active {
  transform: none;
  -webkit-transform: none;
}

