/*********tongyong*********************************************************/

.girlRun {
  width: 9.52%;
  height: 22%;
  top: 65%;
  left: -10%;
  position: absolute;
  z-index: 999;
}

.girl-animate {
  height: 100%;
  width: 100%;
  background: url(../img/girl_run.png) repeat-x;
  background-size: 600% auto;
}

.girlRun.active .girl-animate {
  -webkit-animation: girlRun-action 1s step-end infinite;
  animation: girlRun-action 1s step-end infinite;
}

.girlRun1.active {
  width: 7%;
  left: 45% !important;
}

.girlRun1.active .girl-animate {
  background: url(../img/girl_run1.png) repeat-x;
  background-size: 300% auto;
  background-position: -200% 0;
  -webkit-animation: girlRun1-action 0.3s step-end 1 forwards;
  animation: girlRun1-action 0.3s step-end 1 forwards;
}

.road {
  width: 200%;
  height: 33%;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 2;
}

.road.active {
  -webkit-animation: road-action 13s linear infinite;
  animation: road-action 13s linear infinite;
}

.roadP7.active {
  -webkit-animation: road-action 8s linear 1 forwards;
  animation: road-action 8s linear 1 forwards;
}

.city {
  height: 68%;
  position: absolute;
  left: 0;
  top: 0;
  display: inline-block;
}

.city .cityBg {
  height: 100%;
}

.city.active {
